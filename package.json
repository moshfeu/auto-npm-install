{
  "name": "auto-npm-install",
  "displayName": "Auto NPM install",
  "description": "A plugin to automatic install a missing npm package",
  "version": "0.0.10",
  "publisher": "moshfeu",
  "engines": {
    "vscode": "^1.6.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onDebug"
  ],
  "main": "./out/src/extension",
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "publish": "vsce publish patch",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.12",
    "@types/sinon": "^5.0.7",
    "husky": "^1.2.0",
    "mocha": "^5.2.0",
    "prettier": "^1.15.3",
    "pretty-quick": "^1.8.0",
    "sinon": "^7.1.1",
    "typescript": "^3.2.2",
    "vscode": "^1.1.26"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moshfeu/auto-npm-install.git"
  },
  "keywords": [
    "vscode",
    "extension"
  ],
  "author": "MoshFeu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moshfeu/auto-npm-install/issues"
  },
  "homepage": "https://github.com/moshfeu/auto-npm-install#readme",
  "icon": "assets/icon.jpg",
  "galleryBanner": {
    "color": "#000",
    "theme": "dark"
  },
  "dependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
